public service JSON::to_string(arr: in anonymous sequence of JSONObject) return anonymous string is
delim: string := "";
output_string: string := "";
begin
  output_string := output_string & "[";
  for obj in arr loop
    output_string := output_string & delim & to_string(obj);
    delim := ",";
  end loop;
  output_string := output_string & "]";
  return output_string;
end service;
